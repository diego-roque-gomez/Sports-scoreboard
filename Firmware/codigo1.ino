void handleInicio(){

  if(running){
    server.sendHeader("Location", "/");
    server.send(302);
    return;
  }

  if(!pausa){
    minutos = server.arg("minutes").toInt();
    segundos = server.arg("seconds").toInt();
    tiempo = minutos*60 + segundos;
    if(tiempo == 0){
      cronometro = true;
    }
    else{
      cronometro = false;
    }
  }
  else{
    pausa = false;
  }
  tiempoAnterior = millis();
  running = true;
  
  server.sendHeader("Location", "/");
  server.send(302);

}

void handlePausa(){
  running = false;
  pausa = true;
  
  server.sendHeader("Location", "/");
  server.send(302);
  
}

void handleReset(){
  running = false;
  pausa = false;
  minutos = 0;
  segundos = 0;
  tiempo = 0;
  
  server.sendHeader("Location", "/");
  server.send(302);

}

void handleServo(){
  if (server.method() == HTTP_POST){
    int valor_servo = server.arg("servo").toInt();
    if (valor_servo == 0){
      servo1.write(0);
      servo2.write(0);
      delay(50);
    }
    else if (valor_servo == 1){
      //Desplegar mecanismo
      servo1.write(180);
      servo2.write(180);
      delay(50);
    } 
  }
  server.sendHeader("Location", "/");
  server.send(302);
}

// Toma un número de segundos y los escribe en forma mm:ss en un display de 7 segmentos
void actualizar_display(uint32_t tiempo){
  int minutos = tiempo / 60;
  int segundos = tiempo % 60;

  int digito1 = minutos / 10;
  int digito2 = minutos % 10;
  int digito3 = segundos / 10;
  int digito4 = segundos % 10;

  display.setColumn(7, numerosdisplay[digito1]);
  display.setColumn(6, numerosdisplay[digito2]);
  display.setColumn(5, numerosdisplay[digito3]);
  display.setColumn(4, numerosdisplay[digito4]);

}

// Dibuja la matriz led del puntaje
void dibujarmatriz(int v1){
  if (v1<10){
    matriz.setChar(5, v1+48);
  }
  else if (v1<100){
    int decenas = v1/10;
    int unidades = v1%10;

    for(uint8_t i = 0; i<3; i++){
      matriz.setColumn(i+5, numerosmatriz[decenas][i]);
    }
    for (uint8_t i = 0; i < 3; i++) {
    matriz.setColumn(i, numerosmatriz[unidades][i]);
    }
  }
  else{
    matriz.setChar(5, 48);
  }
}
