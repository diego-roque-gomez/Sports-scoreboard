void setup(){

  //Matriz led
  matriz.begin();
  matriz.control(MD_MAX72XX::INTENSITY, 3);
  matriz.clear();
  matriz.control(MD_MAX72XX::SHUTDOWN, false);

  display.begin();
  display.control(MD_MAX72XX::INTENSITY, 3);
  display.clear();
  display.control(MD_MAX72XX::SHUTDOWN, false);

  servo1.attach(servoPin1);
  servo2.attach(servoPin2);

  servo1.write(90);
  servo2.write(90);

  Serial.begin(112500);

  //Iniciar SoftAP
  WiFi.softAP(ssid, password);
  IPAddress apIP = WiFi.softAPIP();
  Serial.println("IP del AP: ");
  Serial.println(apIP);

  dnsServer.start(53, domain, apIP);

  server.on("/", handleMenu);
  server.on("/actualizar", HTTP_POST, handleForm);
  server.on("/inicio", HTTP_POST, handleInicio);
  server.on("/pausa", HTTP_POST, handlePausa);
  server.on("/reset", HTTP_POST, handleReset);
  server.on("/servo", HTTP_POST, handleServo);

  server.begin();
  Serial.println("Servidor iniciado correctamente");

  delay(1000);

}

void loop(){
  dnsServer.processNextRequest();
  server.handleClient();

  if(running){
    tiempoActual = millis();
    if (tiempoActual - tiempoAnterior >= 1000){
      tiempoAnterior = tiempoActual;
      if(cronometro){
        tiempo++;
      }
      else{
        if(tiempo>0){
        tiempo--;
        }
      }
    }
  actualizar_display(tiempo);
  }

}

